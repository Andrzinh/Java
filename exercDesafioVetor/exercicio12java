public class exercicio12 {
	public static void main(String[] args) {
		
		final int TAM = 10;
		int a[], i;
		a = new int[TAM];

		for (i = 0; i < TAM; i++) {
			a[i] = i + 1;
			if (a[i] == 2) {
				System.out.println("Primo.");
			} else if (a[i] % 2 == 0) {
				System.out.println("Comum.");
			} else {
				System.out.println("Primo.");
			}
		}
	}
}
